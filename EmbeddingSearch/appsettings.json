{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Database=EmbeddingSearch;Username=postgres;Password=your-password;"
  },
  "AzureOpenAI": {
    "Embeddings": {
      "Endpoint": "https://your-azure-openai-endpoint/", // base URL of your Azure OpenAI resource
